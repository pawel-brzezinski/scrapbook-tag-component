{
    "name": "pawel-brzezinski/scrapbook-tag-component",
    "description": "Tag component for Scrapbook Cache library",
    "type": "scrapbook-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Brzeziński",
            "email": "pawel.brzezinski@smartint.pl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PB\\Extension\\Scrapbook\\Tag\\": "src/",
            "PB\\Tests\\Extension\\Scrapbook\\Tag\\": "tests/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "require": {
        "php": "^7.1",
        "psr/cache": "^1.0",
        "matthiasmullie/scrapbook": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
